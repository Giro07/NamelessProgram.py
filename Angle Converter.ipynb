    "def angle_converter(angulo, entrada, saida):\n",
    "    import math\n",
    "    if entrada == \"graus\" and saida == \"rad\":\n",
    "        angulo = math.radians(angulo)\n",
    "    elif entrada == \"rad\" and saida == \"graus\":\n",
    "        angulo = math.degrees(angulo)\n",
    "    return print(angulo,saida)"
   ]
